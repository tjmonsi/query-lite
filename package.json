{
  "name": "@littleq/query-lite",
  "version": "0.1.1",
  "description": "Query Parameters getter. Parses query parameter into an object. Works with @littleq/location-lite",
  "main": "query-lite",
  "scripts": {
    "size": "./node_modules/.bin/size-limit",
    "semistandard": "./node_modules/.bin/semistandard",
    "wct": "npm run pretest && ./node_modules/.bin/wct --npm",
    "wct-firefox": "./node_modules/.bin/wct --npm --configFile wct-firefox.conf.json",
    "wct-prod": "./node_modules/.bin/wct --npm --configFile wct-prod.conf.json",
    "wct-sauce": "./node_modules/.bin/wct --npm --configFile wct-sauce.conf.json",
    "test": "npm run semistandard && npm run size && npm run wct-prod",
    "build": "./node_modules/.bin/rollup -c",
    "pretest": "npm run build",
    "prepublishOnly": "npm test"
  },
  "files": [
    "query-lite.js",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjmonsi/query-lite.git"
  },
  "keywords": [
    "webcomponents"
  ],
  "author": "TJ Monserrat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjmonsi/query-lite/issues"
  },
  "homepage": "https://github.com/tjmonsi/query-lite#readme",
  "flat": true,
  "size-limit": [
    {
      "path": "query-lite.js",
      "limit": "600B"
    }
  ],
  "semistandard": {
    "parser": "babel-eslint",
    "ignore": [
      "test/*",
      "dist/*"
    ],
    "env": [
      "mocha"
    ]
  },
  "devDependencies": {
    "@polymer/iron-test-helpers": "0.0.3",
    "babel-eslint": "8.2.3",
    "chai": "4.1.2",
    "eslint": "4.19.1",
    "eslint-config-semistandard": "12.0.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-html": "3.2.2",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-mocha": "4.12.1",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.1.0",
    "lodash": "^3.10.1",
    "mocha": "5.1.1",
    "rollup": "0.59.1",
    "rollup-plugin-buble": "0.19.2",
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-uglify": "3.0.0",
    "semistandard": "12.0.1",
    "size-limit": "0.18.0",
    "wct-browser-legacy": "0.0.1-pre.11",
    "web-component-tester": "6.5.0"
  }
}
